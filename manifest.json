{

  "manifest_version": 2,
  "name": "mal-fansubs",
  "version": "1.0",

  "description": "Adds fansub section to mal anime pages",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": [
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.myanimelist.net/anime/*"],
      "js": ["fansub.js"]
    }
  ]

}